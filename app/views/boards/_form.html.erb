<%= form_with(model: @board, local: true, class: "ui form") do |f| %>
  <div class="field">
    <%= f.label :name %>
    <%= f.text_field :name, class: "input" %>
  </div>
 
 <div class="field">
   <%= f.label :start_date %>
   <%= f.text_field :start_date, class: "input" %>
 </div>
 <div class="field">
   <%= f.label :finish_date %>
   <%= f.text_field :finish_date, class: "input" %>
 </div>
 <%= f.submit nil, class: "ui blue button" %>
<% end %>